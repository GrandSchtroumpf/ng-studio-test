{
  "hosting": [
    {
      "target": "ng-studio-test",
      "public": "dist/ng-studio-test",
      "ignore": [
        "firebase.json",
        "**/.*",
        "**/node_modules/**"
      ],
      "rewrites": [
        {
          "source": "**",
          "destination": "/index.html"
        }
      ]
    }
  ],
  "emulators": {
    "functions": {
      "port": "5001"
    },
    "firestore": {
      "port": "8080"
    },
    "hosting": {
      "port": "5000"
    },
    "ui": {
      "enabled": true,
      "host": "localhost",
      "port": "4000"
    }
  }
}
